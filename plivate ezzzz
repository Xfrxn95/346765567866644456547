        ["Bench Press"] = CFrame.new(-8177, 60, 1913),
        ["Squat"] = CFrame.new(-8377, 50, 2860),
        ["Boulder"] = CFrame.new(-8617, 6, 2686)
    },

    ["Muscle King Gym"] = {
