local function formatFull(num)
    local formatted = tostring(math.floor(num)):reverse():gsub("(%d%d%d)", "%1,"):reverse()
    return formatted:gsub("^,", "")
end

local function formatCalc(num)
    if num >= 1e30 then return string.format("%.2f", num / 1e30) .. "No"
    elseif num >= 1e27 then return string.format("%.2f", num / 1e27) .. "Oc"
    elseif num >= 1e24 then return string.format("%.2f", num / 1e24) .. "Sp"
    elseif num >= 1e21 then return string.format("%.2f", num / 1e21) .. "Sx"
    elseif num >= 1e18 then return string.format("%.2f", num / 1e18) .. "Qt"
    elseif num >= 1e15 then return string.format("%.2f", num / 1e15) .. "Qd"
    elseif num >= 1e12 then return string.format("%.2f", num / 1e12) .. "T"
    elseif num >= 1e9 then return string.format("%.2f", num / 1e9) .. "B"
    elseif num >= 1e6 then return string.format("%.2f", num / 1e6) .. "M"
    elseif num >= 1e3 then return string.format("%.2f", num / 1e3) .. "K"
    else return tostring(num) end
end

local function getCurrentStrength() return LP.leaderstats.Strength.Value end
local function getCurrentDurability() return LP.Durability.Value end
local function getCurrentRebirths() return LP.leaderstats.Rebirths.Value end
local function getCurrentKills() return LP.leaderstats.Kills.Value end
local function getCurrentBrawls() return LP.leaderstats:FindFirstChild("Brawls") and LP.leaderstats.Brawls.Value or 0 end

local function createLabels(folder)
    return {
        status = folder:AddLabel("Status: Press Start"),
        current = folder:AddLabel("Current: 0"),
        gained = folder:AddLabel("Gained: 0"),
        perMinute = folder:AddLabel("Per Minute: 0"),
        perHour = folder:AddLabel("Per Hour: 0"),
        perDay = folder:AddLabel("Per Day: 0")
    }
end

local strengthLabels = createLabels(strengthFolder)
local durabilityLabels = createLabels(durabilityFolder)
local rebirthsLabels = createLabels(rebirthsFolder2)
local killsLabels = createLabels(killsFolder)
local brawlsLabels = createLabels(brawlsFolder)

_G.strengthActive,_G.durabilityActive,_G.rebirthsActive,_G.killsActive,_G.brawlsActive = false,false,false,false,false
_G.strengthStartTime,_G.strengthStartValue = 0,0
_G.durabilityStartTime,_G.durabilityStartValue = 0,0
_G.rebirthsStartTime,_G.rebirthsStartValue = 0,0
_G.killsStartTime,_G.killsStartValue = 0,0
_G.brawlsStartTime,_G.brawlsStartValue = 0,0

local function updateLabels(active, startTime, startValue, currentValue, labels)
    local currentTime = tick()
    labels.current.Text = "Current: " .. formatCalc(currentValue)
    if active and startTime > 0 then
        local timeElapsed = currentTime - startTime
        local gained = currentValue - startValue
        labels.gained.Text = "Gained: " .. formatCalc(gained)
        if timeElapsed >= 60 then
            local perMinute = (gained / timeElapsed) * 60
            local perHour = perMinute * 60
            local perDay = perHour * 24
            labels.status.Text = "Status: Active"
            labels.perMinute.Text = "Per Minute: " .. formatCalc(perMinute)
            labels.perHour.Text = "Per Hour: " .. formatCalc(perHour)
            labels.perDay.Text = "Per Day: " .. formatCalc(perDay)
        else
            labels.status.Text = "Status: Calculating... " .. math.ceil(60 - timeElapsed) .. "s"
        end
    else
        labels.gained.Text = "Gained: 0"
    end
end

strengthFolder:AddButton("Start Strength Calculator", function() _G.strengthActive = true _G.strengthStartTime = tick() _G.strengthStartValue = getCurrentStrength() end)
strengthFolder:AddButton("Stop Strength Calculator", function() _G.strengthActive = false end)
durabilityFolder:AddButton("Start Durability Calculator", function() _G.durabilityActive = true _G.durabilityStartTime = tick() _G.durabilityStartValue = getCurrentDurability() end)
durabilityFolder:AddButton("Stop Durability Calculator", function() _G.durabilityActive = false end)
rebirthsFolder2:AddButton("Start Rebirths Calculator", function() _G.rebirthsActive = true _G.rebirthsStartTime = tick() _G.rebirthsStartValue = getCurrentRebirths() end)
rebirthsFolder2:AddButton("Stop Rebirths Calculator", function() _G.rebirthsActive = false end)
killsFolder:AddButton("Start Kills Calculator", function() _G.killsActive = true _G.killsStartTime = tick() _G.killsStartValue = getCurrentKills() end)
killsFolder:AddButton("Stop Kills Calculator", function() _G.killsActive = false end)
brawlsFolder:AddButton("Start Brawls Calculator", function() _G.brawlsActive = true _G.brawlsStartTime = tick() _G.brawlsStartValue = getCurrentBrawls() end)
brawlsFolder:AddButton("Stop Brawls Calculator", function() _G.brawlsActive = false end)

task.spawn(function()
    while true do
        updateLabels(_G.strengthActive, _G.strengthStartTime, _G.strengthStartValue, getCurrentStrength(), strengthLabels)
        updateLabels(_G.durabilityActive, _G.durabilityStartTime, _G.durabilityStartValue, getCurrentDurability(), durabilityLabels)
        updateLabels(_G.rebirthsActive, _G.rebirthsStartTime, _G.rebirthsStartValue, getCurrentRebirths(), rebirthsLabels)
        updateLabels(_G.killsActive, _G.killsStartTime, _G.killsStartValue, getCurrentKills(), killsLabels)
        updateLabels(_G.brawlsActive, _G.brawlsStartTime, _G.brawlsStartValue, getCurrentBrawls(), brawlsLabels)
        task.wait(1)
    end
end)

