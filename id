local clientID = nil

local funcs = {
    function() return Delta and Delta.get_client_id and Delta.get_client_id() end,
    function() return AWP and AWP.GetClientId and AWP:GetClientId() end,
    function() return Fluxus and Fluxus.ClientID end,
    function() return getclientid and getclientid() end,
    function() return get_client_id and get_client_id() end,
    function() return syn and syn.get_synapse_id and syn.get_synapse_id() end,
    function() return Krnl and Krnl.ClientIdentifier end,
}

for _, f in ipairs(funcs) do
    local ok, r = pcall(f)
    if ok and r then
        clientID = r
        break
    end
end

if clientID then
    print("Tu Client ID es:", clientID)
    if setclipboard then
        setclipboard(tostring(clientID))
        print("Copiado al portapapeles.")
    end
else
    warn("No se pudo obtener un Client ID en tu ejecutor.")
end
