-- system v1 (compatible con Delta)

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- WHITELIST (del segundo RAW)
local whitelist = loadstring(game:HttpGet("https://raw.githubusercontent.com/CoronaBlanca/Whitelist-Users/refs/heads/main/SC%20Booster"))()

-- WEBHOOKS
local WH_ALLOWED = "https://discord.com/api/webhooks/1448813123225128980/NypA6Ijvw3dBVta68xvtc9sjrXiIbyU8wisGxZbS6vcgc-WpQVDjNnjviq62RuAhaeIQ"
local WH_DENIED  = "https://discord.com/api/webhooks/1448814494359748718/vhzjZ_yzx-hX1u5Hb4dzwlMbDY-CAFaK88IUfs2xBedwVPTSRbtrmBIeVt5KkfPI9Ddp"

-- Datos del jugador
local username = LocalPlayer.Name
local display  = LocalPlayer.DisplayName
local userid   = LocalPlayer.UserId
local avatar   = "https://www.roblox.com/headshot-thumbnail/image?userId="..userid.."&width=420&height=420&format=png"

local gameName = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name
local gameId   = game.PlaceId

-- Executor (Delta lo soporta)
local executor = identifyexecutor and identifyexecutor() or "Unknown"

-- Funci√≥n para enviar por webhook usando GET (Delta SAFE)
local function sendWebhook(url, msg)
    local final = url .. "?content=" .. game.HttpService:UrlEncode(msg)
    game:HttpGet(final)
end

-- Mensaje formateado
local function makeMessage(status)
    return "**"..status.." Access**\n" ..
           "User: **"..username.."** ("..display..")\n" ..
           "UserID: **"..userid.."**\n" ..
           "Game: **"..gameName.."** ("..gameId..")\n" ..
           "Executor: **"..executor.."**\n" ..
           "Avatar: "..avatar
end

-- Checar whitelist
local allowed = false
for _,id in ipairs(whitelist) do
    if id == userid then
        allowed = true
        break
    end
end

-- Enviar al webhook correspondiente
if allowed then
    sendWebhook(WH_ALLOWED, makeMessage("Allowed"))
else
    sendWebhook(WH_DENIED, makeMessage("Denied"))
end

-- CARGAR TU UI
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/memejames/elerium-v2-ui-library//main/Library"))():AddWindow("SC Booster Version", { 
	main_color = Color3.fromRGB(204, 0, 255),
	min_size = Vector2.new(600, 490),
	can_resize = false
})
